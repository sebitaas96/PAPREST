package pruebaSpring.entities;

public class Musico {
	
	private InstrumentoMusical instrumento;
	
	public Musico() {
		//this.trompeta= new Trompeta();
		this.guitarra = new Guitarra();
	}

	
	public void tocar() {
	//	this.trompeta.calentar();
		//this.trompeta.sonar();
		this.guitarra.rasgar();
	}

}
